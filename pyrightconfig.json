{
  "include": [
    "src",
    "model_backends",
    "ui",
    "tests"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.venv",
    "**/venv",
    "**/logs",
    "**/data",
    "**/recordings"
  ],
  "extraPaths": [
    "model_backends/emotion_fatigue_infer",
    "model_backends/emotion_fatigue_infer/emotion",
    "model_backends/emotion_fatigue_infer/fatigue"
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.11",
  "pythonPlatform": "Windows",
  "executionEnvironments": [
    {
      "root": "src",
      "extraPaths": [
        "../model_backends/emotion_fatigue_infer",
        "../model_backends/emotion_fatigue_infer/emotion",
        "../model_backends/emotion_fatigue_infer/fatigue"
      ]
    }
  ]
}
